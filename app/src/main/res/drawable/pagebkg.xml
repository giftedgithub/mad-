<?xml version="1.0" encoding="utf-8"?>
<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Lavender Gradient Background -->
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:startColor="#E6E6FA"
                android:centerColor="#D8BFD8"
                android:endColor="#8A2BE2"
                android:angle="45"/>
        </shape>
    </item>

    <!-- Lavender Abstract Shapes -->
    <item android:right="10dp" android:left="50dp" android:top="20dp" android:bottom="80dp">
        <shape android:shape="oval">
            <solid android:color="#B39EB5"/>
            <size android:width="100dp" android:height="100dp"/>
        </shape>
    </item>

    <item android:right="80dp" android:left="160dp" android:top="150dp" android:bottom="220dp">
        <shape android:shape="oval">
            <solid android:color="#D8BFD8"/>
            <size android:width="120dp" android:height="120dp"/>
        </shape>
    </item>

    <!-- Lavender Circle with Gradient -->
    <item android:right="30dp" android:left="100dp" android:top="250dp" android:bottom="320dp">
        <shape android:shape="oval">
            <gradient
                android:startColor="#D8BFD8"
                android:centerColor="#E6E6FA"
                android:endColor="#8A2BE2"
                android:angle="45"/>
            <size android:width="150dp" android:height="150dp"/>
        </shape>
    </item>

    <!-- Lavender Lines for Texture -->
    <item android:right="0dp" android:left="0dp" android:top="300dp" android:bottom="340dp">
        <shape android:shape="rectangle">
            <solid android:color="#E6E6FA"/>
            <size android:width="5dp" android:height="5dp"/>
        </shape>
    </item>

    <item android:right="0dp" android:left="0dp" android:top="400dp" android:bottom="440dp">
        <shape android:shape="rectangle">
            <solid android:color="#B39EB5"/>
            <size android:width="5dp" android:height="5dp"/>
        </shape>
    </item>

    <!-- Lavender Rounded Rectangle -->
    <item android:right="40dp" android:left="130dp" android:top="350dp" android:bottom="430dp">
        <shape android:shape="rectangle">
            <solid android:color="#8A2BE2"/>
            <corners android:radius="20dp"/>
            <size android:width="80dp" android:height="50dp"/>
        </shape>
    </item>

</layer-list>
